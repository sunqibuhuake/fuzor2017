                    </div>
                </div>
            </section>
            <section class="box ">
                <header class="panel_header">
                    <h2 class="title pull-left">最多收藏</h2>
                </header>
                <div class="content-body">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="row">
                                <% collection.forEach(function(model) { %>
                                <%- include('components/discoverModel', {model: model}) %>
                                <% }) %>
                            </div>

                        </div>
                    </div>
                </div>
            </section>

        </div>
    </section>
