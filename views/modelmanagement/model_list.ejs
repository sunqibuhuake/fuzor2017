<table class="table table-actions-bar">
    <thead>
    <tr>
        <th>截图</th>
        <th>名称</th>
        <th>类型</th>
        <th>上传日期</th>
        <th>管理</th>
    </tr>
    </thead>
    <tbody>
    <% models.forEach(function(model){ %>
    <tr>
        <td><img src="/img/<%= model.snap %>" class="thumb-sm" alt=""> </td>
        <td><%= model.name%></td>
        <td>
            <%= model.catName %>
        </td>
        <td><%= model.date %></td>
        <td>
            <a href="/models/<%= model._id %>" class="table-action-btn"><i class="md md-edit"></i></a>
        </td>
    </tr>
    <% }) %>
    </tbody>
</table>
