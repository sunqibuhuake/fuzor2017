<%- include('global/xheader') %>
<section id="main-content" class=" ">
    <section class="wrapper" style='margin-top:60px;display:inline-block;width:100%;padding:15px 0 0 15px;'>


        <div class="clearfix"></div>

        <div class="col-lg-12">
            <section class="box ">
                <header class="panel_header">
                    <h2 class="title pull-left">面板</h2>
                </header>
                <div class="content-body">
                    <div class="row">

                        <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                            <div class="tile-counter bg-primary">
                                <div class="content">
                                    <i class="fa fa-cube icon-lg"></i>
                                    <h2 class="number_counter start_timer counted" data-speed="3000" data-from="100" data-to="<%= origin %>"><%= origin %></h2>
                                    <div class="clearfix"></div>
                                    <span>上传模型</span>
                                </div>
                            </div>

                        </div>


                        <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                            <div class="tile-counter bg-orange">
                                <div class="content">
                                    <i class="fa fa-share icon-lg"></i>
                                    <h2 class="number_counter start_timer counted" data-speed="3000" data-from="100" data-to="<%= sharing %>"><%= sharing %></h2>
                                    <div class="clearfix"></div>
                                    <span>分享模型</span>
                                </div>
                            </div>

                        </div>

                        <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12">
                            <div class="tile-counter bg-info">
                                <div class="content">
                                    <i class="fa fa-star-o icon-lg"></i>
                                    <h2 class="number_counter start_timer counted" data-speed="3000" data-from="100" data-to="<%= collection %>"><%= collection %></h2>
                                    <div class="clearfix"></div>
                                    <span>收藏模型</span>
                                </div>
                            </div>

                        </div>

                </div>
            </section>

            <section class="box ">
                <header class="panel_header">
                    <h2 class="title pull-left">最新模型</h2>
                </header>
                <div class="content-body">
                    <div class="row">
                        <% last.forEach(function(model) { %>
                        <%- include('components/modelItem', {model: model}) %>
                        <% }) %>
                    </div>
            </section>
        </div>
    </section>
</section>
<%- include('global/xfooter') %>